@{
    ViewData["Title"] = "Home Page";
}

@section Styles{

<link href="~/lib/datatable/css/datatable-bootstrap.css" rel="stylesheet" />
}




<div class="container">
    <br />
    <div style="width:90%; margin:0 auto;">
        <table id="customerDatatable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Contact</th>
                    <th>Email</th>
                    <th>Date Of Birth</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Scripts{
    <script src="~/lib/datatable/js/datatable.jquery.min.js"></script>
    <script src="~/lib/datatable/js/datatable.min.js"></script>
    <script src="~/js/customerdatatable.js"></script>
}